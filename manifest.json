{
    "display_information": {
        "name": "Saks-Slack",
        "description": "A Cortex Analyst implementation via Slack",
        "background_color": "#000000",
        "long_description": "Instantly access Snowflake insights right from Slack. Our app lets you query your data using natural language, getting answers and charts without leaving your team's conversations. Boost productivity and democratize data access for everyone."
    },
    "features": {
        "bot_user": {
            "display_name": "SAKS SLACK",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "channels:read",
                "groups:read",
                "chat:write",
                "groups:history",
                "im:history",
                "mpim:history",
                "files:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "message.channels",
                "message.groups",
                "message.im",
                "message.mpim"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}